<h2 style="font-style:@FontStyle">
	@Text
	@ChildContent
</h2>

@code {
	[Parameter] public string FontStyle { get; set; }
	[Parameter] public string Text { get; set; }
	[Parameter] public RenderFragment ChildContent { get; set; }
}


